                    <option>Selecione</option>
                </select>

            </div>
            <div class="form-group">
                <label for="exampleFormControl" class="control-label">SubTipo</label>
                <input asp-for="SubTipoServico.Nome" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

