                @Html.DisplayFor(modelItem => modelItem.TipoServico.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => modelItem.SubTipoServico.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => modelItem.Observacao)
            </td>
