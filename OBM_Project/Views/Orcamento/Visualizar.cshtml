</div>

<table class="table">
    <thead>
        <tr>
            <th>
                N° Orçamento
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoServico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubTipoServico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Observacao)
            </th>
